{
  "name": "real-teeth",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev:prod": "vite --mode prod --host",
    "dev:dev": "vite --mode dev --host",
    "build:prod": "tsc -b && vite build --mode prod",
    "build:dev": "tsc -b && vite build --mode dev",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest --watch --ui",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "prepare": "husky",
    "check": "pnpm run lint && pnpm run type-check && pnpm run test && pnpm run build:dev",
    "check:quick": "pnpm run lint && pnpm run type-check && pnpm run test"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "lucide-react": "^0.541.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^4.0.13",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.6.3",
    "tailwindcss": "^4.1.11",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^22.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "3.2.4",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "msw": "^2.10.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
